rdAreaGroup, isStatic: true });\n}\n\n// contact with these bodies causes pinball to be relaunched\nexport function ResetAreaFactory(x: number, width: number) {\n  return Matter.Bodies.rectangle(x, 781, width, 2, {\n    label: \"reset\",\n    isStatic: true,\n    render: {\n      fillStyle: \"#fff\",\n    },\n  });\n}\n","/Users/jovicheng/Desktop/trillion-rapier/src/utils/index.ts",[],"/Users/jovicheng/Desktop/trillion-rapier/src/types/index.ts",[],"/Users/jovicheng/Desktop/trillion-rapier/src/const/index.ts",[],"/Users/jovicheng/Desktop/trillion-rapier/src/components/TrillonRapier.tsx",["56","57","58"],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":14,"column":24,"nodeType":"65","messageId":"66","endLine":14,"endColumn":28},{"ruleId":"63","severity":1,"message":"67","line":14,"column":30,"nodeType":"65","messageId":"66","endLine":14,"endColumn":39},{"ruleId":"63","severity":1,"message":"68","line":43,"column":9,"nodeType":"65","messageId":"66","endLine":43,"endColumn":21},{"ruleId":"63","severity":1,"message":"69","line":1,"column":36,"nodeType":"65","messageId":"66","endLine":1,"endColumn":44},{"ruleId":"63","severity":1,"message":"70","line":29,"column":7,"nodeType":"65","messageId":"66","endLine":29,"endColumn":17},{"ruleId":"63","severity":1,"message":"67","line":29,"column":25,"nodeType":"65","messageId":"66","endLine":29,"endColumn":34},"no-native-reassign",["71"],"no-negated-in-lhs",["72"],"@typescript-eslint/no-unused-vars","'ball' is assigned a value but never used.","Identifier","unusedVar","'hitsCount' is assigned a value but never used.","'PINBALL_SIZE' is assigned a value but never used.","'useState' is defined but never used.","'totalScore' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]